<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="15dp">

    <TextView
        android:id="@+id/dummytext1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:drawablePadding="8dp"
        android:drawableTint="@color/colorBlueDark"
        android:fontFamily="@font/gg_regular"
        android:gravity="center_vertical"
        android:padding="10dp"
        android:text="@string/app_name"
        android:textColor="@color/colorBlueDark"
        android:textSize="14dp"
        tools:ignore="HardcodedText,SpUsage" />

    <TextView
        android:id="@+id/dummytext2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/dummytext1"
        android:layout_margin="2dp"
        android:drawablePadding="8dp"
        android:drawableTint="@color/colorBlueDark"
        android:fontFamily="@font/gg_regular"
        android:gravity="center_vertical"
        android:padding="10dp"
        android:text="@string/app_name"
        android:textColor="@color/colorBlueDark"
        android:textSize="14dp"
        tools:ignore="HardcodedText,SpUsage" />

    <TextView
        android:id="@+id/dummytext3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/dummytext2"
        android:layout_margin="2dp"
        android:drawablePadding="8dp"
        android:drawableTint="@color/colorBlueDark"
        android:fontFamily="@font/gg_regular"
        android:gravity="center_vertical"
        android:padding="10dp"
        android:text="@string/app_name"
        android:textColor="@color/colorBlueDark"
        android:textSize="14dp"
        tools:ignore="HardcodedText,SpUsage" />
</RelativeLayout>